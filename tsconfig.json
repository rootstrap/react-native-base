{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "jsx": "react",
      "outDir": "./dist",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "skipLibCheck": true,
      "declaration": true,
      "noUnusedLocals": true,
      "suppressImplicitAnyIndexErrors": true,
      "baseUrl": "./",
      "paths": {
        "*": [
          "src/*"
        ],
        "actions/*": [
          "src/actions/*"
        ],
        "httpClient/*": [
          "src/httpClient/*"
        ],
        "components/*": [
          "src/components/*"
        ],
        "@config/*": [
          "src/config/*"
        ],
        "screens/*": [
          "src/screens/*"
        ],
        "hooks/*": [
          "src/hooks/*"
        ],
        "locale/*": [
          "src/locale/*"
        ],
        "reducers/*": [
          "src/reducers/*"
        ],
        "selectors/*": [
          "src/selectors/*"
        ],
        "store/*": [
          "src/store/*"
        ],
        "utils/*": [
          "src/utils/*"
        ],
        "navigators/*": [
          "src/navigators/*"
        ],
        "services/*": [
          "src/services/*"
        ],
        "validations/*": [
          "src/validations/*"
        ]
      }
    },
    "exclude": [
      "node_modules",
      "__tests__/**/*-test.ts"
    ]
  }