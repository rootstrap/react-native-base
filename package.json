{
    "name": "react-native-base",
    "version": "0.0.2-4",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BrianJVarley/easy-stocks.git"
    },
    "scripts": {
        "lint": "tslint -p \".\" -t stylish",
        "lint:advanced": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
        "lint:fix": "tslint --fix src",
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "ios:dev": "react-native run-ios --scheme ReactNativeBase-Develop",
        "ios:staging": "react-native run-ios --scheme ReactNativeBase-Staging",
        "ios:prod": "react-native run-ios --scheme ReactNativeBase-Prod",
        "android:dev": "react-native run-android --variant=devDebug --appIdSuffix=dev",
        "android:staging": "react-native run-android --variant=stagingDebug --appIdSuffix=staging",
        "android:prod": "react-native run-android --variant=prodDebug",
        "android:release:dev": "cd android && ./gradlew assembleDevRelease",
        "android:release:debug": "cd android && ./gradlew assembleDevDebug",
        "android:release:staging": "cd android && ./gradlew assembleStagingRelease",
        "android:release:prod": "cd android && ./gradlew assembleProdRelease",
        "android:bundle:dev": "cd android && ./gradlew bundleDevRelease",
        "android:bundle:debug": "cd android && ./gradlew bundleDevDebug",
        "android:bundle:staging": "cd android && ./gradlew bundleStagingRelease",
        "android:bundle:prod": "cd android && ./gradlew bundleProdRelease",
        "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
        "bump": "./rnbv.js",
        "test": "jest",
        "test:cover": "yarn run test --coverage",
        "prettier": "prettier --write './src/**/*.ts'",
        "precommit": "lint-staged",
        "publish:app": "np",
        "postversion": "react-native-version --target android"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.14.1",
        "@react-navigation/native": "5.0.0",
        "@react-navigation/stack": "5.0.0",
        "@rootstrap/redux-tools": "^0.4.2",
        "@rootstrap/validate": "^1.0.0",
        "@types/humps": "^2.0.0",
        "@types/jest": "^26.0.20",
        "@types/lodash": "^4.14.168",
        "@types/react": "^17.0.3",
        "@types/react-native": "^0.64.2",
        "@types/react-test-renderer": "^17.0.1",
        "@types/redux-logger": "^3.0.8",
        "axios": "^0.19.2",
        "deprecated-react-native-prop-types": "^2.3.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-plugin-prettier": "3.1.0",
        "fast-memoize": "^2.5.2",
        "humps": "2.0.1",
        "immer": "5.0.0",
        "immutability-helper": "^3.1.1",
        "install": "^0.13.0",
        "jest-environment-jsdom": "^27.1.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "native-base": "^2.15.0",
        "pkg-dir": "^5.0.0",
        "prop-types": "15.7.2",
        "react": "16.13.1",
        "react-native": "0.63.2",
        "react-native-chart-kit": "^6.11.0",
        "react-native-config": "0.12.0",
        "react-native-elements": "^3.4.2",
        "react-native-gesture-handler": "^1.5.6",
        "react-native-localization": "2.1.5",
        "react-native-multiple-select": "^0.5.9",
        "react-native-reanimated": "^1.7.0",
        "react-native-safe-area-context": "^0.7.2",
        "react-native-screens": "^2.0.0-beta.2",
        "react-native-super-grid": "^4.1.3",
        "react-native-svg": "^12.1.0",
        "react-native-typescript-transformer": "^1.2.13",
        "react-native-vector-icons": "^8.1.0",
        "react-redux": "7.1.1",
        "redux": "4.0.4",
        "redux-flipper": "^1.3.1",
        "redux-persist": "^6.0.0",
        "save": "^2.4.0",
        "ts-jest": "^26.5.4",
        "tslint": "^6.1.3",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-react": "^5.0.0",
        "tslint-react-recommended": "^1.0.15",
        "typescript": "^4.2.3"
    },
    "devDependencies": {
        "@babel/core": "7.8.4",
        "@babel/preset-env": "^7.13.12",
        "@babel/runtime": "7.8.4",
        "@react-native-community/eslint-config": "1.1.0",
        "@testing-library/react-native": "7.0.0",
        "@types/react-grid-layout": "^1.1.1",
        "@types/react-navigation": "^3.4.0",
        "@types/react-redux": "^7.1.16",
        "@types/redux-persist": "^4.3.1",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.19.0",
        "axios-mock-adapter": "^1.17.0",
        "babel-jest": "^25.1.0",
        "babel-plugin-module-resolver": "3.2.0",
        "babel-plugin-react-native-classname-to-style": "^1.2.2",
        "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
        "enzyme-adapter-react-16": "1.14.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "17.1.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-babel-module": "5.1.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "1.6.1",
        "husky": "^8.0.2",
        "jest": "^26.6.3",
        "lint-staged": "^13.0.3",
        "metro-react-native-babel-preset": "0.59.0",
        "np": "^7.6.2",
        "postcss": "^8.3.0",
        "prettier": "^2.3.2",
        "react-dom": "16.9.0",
        "react-native-flipper": "^0.54.0",
        "react-native-postcss-transformer": "^1.2.4",
        "react-native-rename": "^2.8.1",
        "react-native-version": "^4.0.0",
        "react-test-renderer": "^17.0.1",
        "redux-logger": "3.0.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "yarn lint:fix --quiet",
            "git add"
        ]
    }
}
