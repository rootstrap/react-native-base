{
  "name": "react-native-base",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios --scheme ReactNativeBase",
    "ios:develop": "react-native run-ios --scheme ReactNativeBase-Develop",
    "ios:staging": "react-native run-ios --scheme ReactNativeBase-Staging",
    "test": "jest --silent",
    "test:cover": "yarn run test --coverage",
    "prettier": "prettier --write './src/**/*.js'"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.5.1",
    "eslint-plugin-prettier": "3.1.0",
    "humps": "^2.0.1",
    "i18n-js": "^3.2.1",
    "immer": "^3.1.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "query-string": "^6.5.0",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-config": "^0.11.7",
    "react-native-localize": "^1.1.4",
    "react-native-navigation": "^3.0.0-alpha.6",
    "react-redux": "7.1.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.3",
    "redux-react-native-session": "^1.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.2.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "nock": "^10.0.6",
    "prettier": "^1.18.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "redux-logger": "^3.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "yarn lint:fix --quiet",
      "git add"
    ]
  }
}
